{
  "name": "@d-matrix/icons-react",
  "version": "0.0.5",
  "type": "module",
  "files": [
    "dist",
    "readme.md"
  ],
  "main": "./dist/icons-react.cjs",
  "module": "./dist/icons-react.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/icons-react.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/icons-react.cjs"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./src/components",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "pregenerate": "npm run clean",
    "generate": "svgr --out-dir ./src/components --typescript -- ./src/assets",
    "postgenerate": "npm run build",
    "prepublishOnly": "npm run build",
    "wait3s": "node -e \"setTimeout(() => process.exit(0), 3000)\"",
    "cnpm:sync": "cnpm sync %npm_package_name%",
    "postpublish": "echo \"wait for 3 seconds, then sync cnpm\" && npm run wait3s && npm run cnpm:sync"
  },
  "keywords": [
    "react",
    "icons",
    "svg"
  ],
  "author": "Lin Du",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@eslint/js": "^9.15.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@svgr/cli": "^8.1.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "cnpm": "^9.4.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
