{
  "name": "@d-matrix/icons-react",
  "version": "0.56.3",
  "type": "module",
  "files": [
    "dist",
    "readme.md",
    "package.json"
  ],
  "sideEffects": false,
  "main": "./dist/icons/index.js",
  "module": "./dist/icons/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/icons/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/icons/index.js"
      }
    },
    "./package.json": "./package.json",
    "./utils": {
      "import": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.mjs"
      },
      "require": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.js"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrdulin/dm-icons.git"
  },
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf ./src/components ./dist",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "pregenerate": "npm run clean",
    "generate:step2": "rimraf ./src/icons && cross-env TS_NODE_PROJECT=scripts/tsconfig.json node --import ./scripts/ts-node.register.mjs scripts/generate.ts",
    "generate:step1": "svgr --out-dir ./src/components --typescript -- ./src/assets",
    "generate": "npm run generate:step1 && npm run generate:step2",
    "postgenerate": "npm run build",
    "prepublishOnly": "npm run generate && npm run build",
    "wait3s": "node -e \"setTimeout(() => process.exit(0), 3000)\"",
    "cnpm:sync": "cnpm sync %npm_package_name%",
    "postpublish": "echo \"wait for 3 seconds, then sync cnpm\" && npm run wait3s && npm run cnpm:sync",
    "load": "dotenvx run -f .env -- sh -c 'echo $NPM_AUTHTOKEN'",
    "pb": "dotenvx run -f .env -- npm publish --access public",
    "postversion": "npm run update:deps --workspace=@d-matrix/icons-site"
  },
  "keywords": [
    "react",
    "icons",
    "svg"
  ],
  "author": "Lin Du",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@dotenvx/dotenvx": "^1.51.2",
    "@eslint/js": "^9.15.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@svgr/cli": "^8.1.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "cnpm": "^9.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "glob": "^10.4.5",
    "globals": "^15.12.0",
    "lodash": "^4.17.21",
    "rimraf": "^5.0.10",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.3.0"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "rc-util": "^5.44.3"
  }
}